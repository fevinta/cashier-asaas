includes:
    - vendor/larastan/larastan/extension.neon

parameters:
    level: 4
    paths:
        - src
    tmpDir: build/phpstan
    ignoreErrors:
        - '#Trait .+ is used zero times and is not analysed#'
        - '#Access to an undefined property .+::\$#'
        - '#Call to an undefined method .+::#'
    excludePaths:
        - src/Events
